{
  "hosting": {
    "public": "dist/foxflow/browser",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/assets/i18n/**",
        "destination": "/assets/i18n/**"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}

